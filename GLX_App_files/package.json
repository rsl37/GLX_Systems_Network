  "scripts": {
    "build": "vite build && tsc --project tsconfig.server.json",
    "build:production": "NODE_ENV=production npm run build",
    "start": "npx tsx watch scripts/dev.ts",
    "setup:env": "scripts/setup-env.sh",
    "test": "scripts/ensure-test-dirs.sh && vitest run",
    "test:api": "scripts/ensure-test-dirs.sh && vitest --run tests/api",
    "test:socket": "scripts/ensure-test-dirs.sh && vitest --run tests/socket",
    "test:security": "scripts/ensure-test-dirs.sh && vitest --run tests/security",
    "test:watch": "scripts/ensure-test-dirs.sh && vitest",
    "test:coverage": "scripts/ensure-test-dirs.sh && vitest run --coverage",
    "test:ui": "scripts/ensure-test-dirs.sh && vitest --ui",
    "test:e2e": "scripts/run-e2e-tests.sh",
    "test:e2e:force": "scripts/ensure-test-dirs.sh && playwright test",
    "test:all": "npm run test && npm run test:security && npm run test:e2e",
    "test:env": "npx tsx scripts/test-env.ts",
    "test:services": "npx tsx scripts/test-service-connectivity.ts",
    "deployment:check": "npx tsx scripts/deployment-check.ts",
    "deployment:test": "npx tsx scripts/test-deployment-validation.ts",
    "production:setup": "npx tsx scripts/production-setup.js",
    "production:check": "NODE_ENV=production npx tsx scripts/deployment-check.js",
    "domain:check": "npx tsx scripts/domain-check.ts",
    "health:log": "npx tsx scripts/health-location-status-logger.ts",
    "branch:analyze": "npx tsx scripts/branch-commit-analyzer.ts",
    "logs:all": "npm run health:log && npm run branch:analyze",
    "dashboard": "npx tsx scripts/monitoring-dashboard.ts",
    "monitor:full": "npm run logs:all && npm run dashboard",
    "vercel:validate": "node scripts/validate-vercel-env.cjs",
    "vercel:health": "node scripts/vercel-health-check.cjs",
    "vercel:deploy-check": "npm run vercel:validate && npm run build && npm run vercel:health",
    "vercel:troubleshoot": "node scripts/vercel-troubleshoot.cjs",
    "blockchain:compile": "hardhat compile",
    "blockchain:test": "hardhat test",
    "blockchain:deploy": "hardhat run scripts-hardhat/deploy.ts",
    "blockchain:deploy:localhost": "hardhat run scripts-hardhat/deploy.ts --network localhost",
    "blockchain:deploy:sepolia": "hardhat run scripts-hardhat/deploy.ts --network sepolia",
    "blockchain:node": "hardhat node",
    "blockchain:clean": "hardhat clean"
  },
